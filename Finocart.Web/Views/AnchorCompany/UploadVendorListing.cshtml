@model Finocart.CustomModel.VendorAwaitedInvApprovalModel
@{
    ViewData["Title"] = "User";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/AnchorCompany/UploadVendorList.js"></script>
<script>
    var GetUploadData = '@Url.Action("GetUploadVendorList", "AnchorCompany")';
</script>
<div class="main-inner-section">
    <div class="content-upper-section">
        <div class="tabsec-main">
            <div class="tab-content">
                <div id="eInvoices" class="tab-pane fade in active">
                    <div class="row" id="eAnchorCompInvoices">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                           
                            <div class="search-box-main clearfix">
                                <h4 style="font-size:15px;font-weight:bold;">Vendor Company: <span style="" id="lblcompanyName"></span></h4>
                                <div class="col-xs-12 col-sm-1 col-md-1 padding">
                                    <a class="excelbutton" href="javascript:history.back()" style="margin: 0;background-color: #277dc7;border-color: #277dc7;float: left;line-height: 17px;">Back</a>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-4 padding">
                                    @Html.TextBox("txt_PONumber", null, new { placeholder = "Search by PO NUMBER", @class = "searchTerm" })<span class="searchButton"><img src="~/Content/images/searchicon.png" class="img-responsive" style="display: inline;" /></span>
                                </div>
                                @*<div class="col-xs-12 col-sm-4 col-md-4 padding">
                                    @Html.TextBox("txt_InvoiceNo", null, new { placeholder = "Search by INVOICE NO", @class = "searchTerm" })<span class="searchButton"><img src="~/Content/images/searchicon.png" class="img-responsive" style="display: inline;" /></span>
                                </div>*@

                                <div class="col-xs-12 col-sm-7 col-md-7 padding">
                                    <button id="ExportAnchCompCSV" class="excelbutton" style="margin-right:0;">
                                        <img src="~/Content/images/excelicon.png" title="Download Excel File" class="img-responsive" style="display: inline;" />
                                    </button>

                                </div>
                            </div>
                        </div>

                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="main-table-box page-scroll">
                                <div class="table-responsive">
                                    @Html.HiddenFor(m => m.VendorID)
                                    @Html.HiddenFor(m => m.VendorName, new { @id = "lblVendorCompany" })
                                    

                                    <table id="tbl_UploadVendorList" class="table table-responsive table-hover table-bordered tbl_UserList">
                                        <thead>
                                            <tr>
                                                @*<th style="width:10%">Vendor Company</th>*@
                                                <th style="width:10%">PO Number</th>
                                                <th style="width:10%">Invoice Date</th>
                                                <th style="width:13%">Invoice Number</th>
                                                <th style="width:12%">Invoice Amount(INR)</th>
                                                <th style="width:10%">Payment Due Date</th>
                                                <th style="width:20%">Approved Amount(INR)</th>
                                                <th style="width:20%">Invoice Status</th>

                                                @*<th style="width:15%">Intrested As</th>*@
                                                <th style="width:10%">Payment Days</th>
                                                <th style="width:10%">Payment Date</th>


                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

