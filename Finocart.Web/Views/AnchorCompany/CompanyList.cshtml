@model Finocart.Model.Company
@{
    ViewData["Title"] = "User";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/AnchorCompany/CompanyList.js"></script>
<script>
    var GetCompanyData = '@Url.Action("GetCompanyData", "AnchorCompany")';
</script>
<div class="main-inner-section">
    <div class="content-upper-section">
        <div class="tabsec-main">
            <div class="tab-content">
                <div id="eInvoices" class="tab-pane fade in active">
                    <div class="row" id="eAnchorCompInvoices">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="search-box-main clearfix">
                                <h4 style="font-size:15px;font-weight:bold;">Factoring</h4>
                                <div class="col-xs-12 col-sm-4 col-md-4 padding">
                                    @Html.TextBox("txt_CompanyNameSearch", null, new { placeholder = "Search", @class = "searchTerm" })<span class="searchButton"><img src="~/Content/images/searchicon.png" class="img-responsive" style="display: inline;" /></span>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-md-4 padding">
                                    @Html.DropDownListFor(m => m.InterestedAs, new SelectList(ViewBag.LookUp, "ID", "LookUpValue"), "Select", new { @class = "searchTerm" })
                                </div>

                                <div class="col-xs-12 col-sm-4 col-md-4 padding">
                                    <button id="ExportAnchCompCSV" class="excelbutton" style="margin-right:0;">
                                        <img src="~/Content/images/excelicon.png" title="Download Excel File" class="img-responsive" style="display: inline;" />
                                    </button>
                                    @*<button id="btnComapnyFilter" class="filterbtn">
                <img src="~/Content/images/filtericon.png" title="Filter" class="img-responsive" />
            </button>*@
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="main-table-box page-scroll">
                                <div class="table-responsive">
                                    <table id="tbl_CompanyList" class="table table-responsive table-hover table-bordered">
                                        <thead>
                                            <tr>
                                                <th>Company Names</th>
                                                <th>Contact Person</th>
                                                <th>Mobile No.</th>
                                                <th>Email ID</th>
                                                <th>PAN No.</th>
                                                <th>CIN No.</th>
                                                <th>Intrested As</th>
                                                <th>Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            
        </div>
    </div>
    </div>
</div>
<script type="text/javascript">

    var AddResult = '@(TempData["success"])';
    if (AddResult == 'success') {
        debugger;
        //alert("New User Added Successfully.Email Notification have been sent with Company ID and Registration Llink Using which User will able to Register with Portal and Set Password for Future Portal Usage");
        Common.CustomSuccessNotify("New User Added Successfully.Email Notification have been sent with Company ID and Registration Llink Using which User will able to Register with Portal and Set Password for Future Portal Usage");
    }
</script>


