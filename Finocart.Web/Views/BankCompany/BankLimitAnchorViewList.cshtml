@model Finocart.CustomModel.BankLimitAnchorListView
@{
    ViewData["Title"] = "BankLimitAnchorViewList";
}
<style>
    .hide_column {
        display: none;
    }

    .backbtn {
        display: inline-block;
        padding-top: 0;
        padding-bottom: 0;
    }
</style>
<script src="~/BankCompany/BankLimitAnchorViewList.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/css/bootstrap-datepicker3.css" />
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.4.1/js/bootstrap-datepicker.min.js"></script>
<script>
    var GetBankLimitAnchorViewList = '@Url.Action("GetBankLimitAnchorViewList", "BankCompany")';
    var TeamDetailPostBackURL = '@Url.Action("EditBankAnchorDetail", "BankCompany")';
    var GetBankLimitLogList = '@Url.Action("GetBankLimitLogList", "BankCompany")';
</script>
<div class="main-inner-section">
    <div class="content-upper-section">
        <div class="tabsec-main">
            <div class="tab-content">
                <div id="eInvoices" class="tab-pane fade in active">
                    <div class="row" id="eAnchorCompInvoices">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="search-box-main clearfix">
                                <a href="javascript:history.back()"><i class="fas fa-long-arrow-alt-left" title="Go Back"></i></a>
                                <h4 style="font-size:15px;font-weight:bold;">LIMIT</h4>
                                <div class="col-xs-12 col-sm-4 col-md-4 padding">
                                    @Html.TextBox("txt_AnchorNameSearch", null, new { placeholder = "Search", @class = "searchTerm" })<span class="searchButton"><img src="~/Content/images/searchicon.png" class="img-responsive" style="display: inline;" /></span>
                                    <label>@Model.Company_name</label>
                                </div>


                                <div class="col-xs-12 col-sm-8 col-md-8 padding">
                                    <button id="ExportAnchCompCSV" class="excelbutton" style="margin-right:0;">
                                        <img src="~/Content/images/excelicon.png" title="Download Excel File" class="img-responsive" style="display: inline;" />
                                    </button>

                                    <button id="btnAddNewSetLimit" class="adduserbutton" style="width:200px;float:right;margin-right:15px;">
                                        <img src="~/Content/images/plusicon.png" class="img-responsive" style="display: inline;" />Add New Set Limit
                                    </button>
                                </div>
                            </div>

                        <div class="">
                            <div class="main-table-box page-scroll">
                                <div class="table-responsive">
                                    <input type="hidden" id="hdnCompanyID" value="@Model.CompanyID" />
                                    <input type="hidden" id="hdnCompanyName" value="@Model.Company_name" />
                                    <input type="hidden" id="hdnCompanyEmail" value="@Model.Contact_email" />
                                    <table id="tbl_BankLimitAnchorViewList" class="table table-responsive table-hover table-bordered">
                                        <thead>
                                            <tr>
                                                <th hidden>ID</th>
                                                @*<th>Anchor Name</th>
                                                <th>Contact Name</th>
                                                <th>Anchor Email</th>
                                                <th>Contact Number</th>*@
                                                <th>Available Limits (INR)</th>
                                                <th>Utilized Limits (INR)</th>
                                                <th>ODBD</th>
                                                <th>Interest Rate (% P.A.)</th>
                                                <th>Validity From Date</th>
                                                <th>Validity Upto</th>
                                                <th>Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
<div id='myModal' class='modal'>
    <div class="modal-dialog">
        <div class="modal-content">
            <div id='myModalContent'></div>
        </div>
    </div>
</div>

<div id="frview" class="modal fade frview" role="dialog">
    <div class="modal-dialog" style="width:800px;">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close new-close" data-dismiss="modal">×</button>
            </div>
            <div class="modal-body">
                <div class="frview-tab">
                    <div id="dvFundingReqAmount">
                        <table class="table table-responsive" id="tblBankSetLimitLogView">
                            <thead>
                                <tr>
                                    @*<th hidden>ID</th>*@
                                    @*<th>Anchor Name</th>
                                    <th>Contact Name</th>
                                    <th>Anchor Email</th>
                                    <th>Contact Number</th>*@
                                    <th>Available Limits (INR)</th>
                                    <th>Utilized Limits (INR)</th>
                                    <th>ODBD</th>
                                    <th>Interest Rate (% P.A.)</th>
                                    <th>Validity From Date</th>
                                    <th>Validity Upto</th>
                                    <th>Modified Date</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

